{
  "name": "dbt/model-factory",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Dbt\\ModelFactory\\": "src/"
    }
  },
  "require": {
    "php" : ">=7.1.3",
    "illuminate/support": "~5.8.0",
    "illuminate/database": "~5.8.0",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0",
    "dbt/phpunit-runner": "~1.0.0",
    "vimeo/psalm": "^3.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Dbt\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "php ./vendor/dbt/phpunit-runner/phpunit.php --no-coverage",
    "coverage": "phpunit --coverage-html coverage",
    "pcov": "php -d pcov.enabled=1 vendor/phpunit/phpunit/phpunit --coverage-text --coverage-html coverage",
    "stat": "psalm"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dbt\\ModelFactory\\ModelFactoryProvider"
      ]
    }
  }
}
